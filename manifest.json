{
  "manifest_version": 3,
  "name": "Auto Next",
  "version": "1.0",
  "description": "Automatically plays the next episode on a specific media player website.",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "*://yovivo.co/*"
  ],
  "icons": {
  "16": "icons/icon16.png",
  "32": "icons/icon32.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png"
},
  "content_scripts": [
    {
      "matches": [
        "*://yovivo.co/watch-tv/watch-how-i-met-your-mother-movies-free-hd-online-vumoo-*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}